    {{!-- contact us section --}}

    <section id="contact_us_section" class="py-5 background1">
      <div class="container">
        <h3 class="text-center text-muted">Contact Us</h3>
        <div class="row">
          <div class="col-md-12">



  <form action="/process-contact-form"method="post">
  <div class="form-group">
    <label for="exampleInputEmail1">Your Email address</label>
    <input name="email"type="email" class="form-control" id="exampleInputEmail1" aria-describedby="emailHelp">
    <small id="emailHelp" class="form-text text-muted ">We'll never share your email with anyone else.</small>
  </div>
  <div class="form-group">
    <label for="exampleInputPassword1">Your Contact number</label>
    <input name="phone"type="number" class="form-control" id="contactNumberField">
  </div>

  <div class="mb-3">
    <label for="validationTextarea">Textarea</label>
    <textarea name="query" style="height:250px;"class="form-control is-invalid" id="validationTextarea" placeholder="Enter your query here" required></textarea>
    <div class="invalid-feedback">
      Please enter a message in the textarea.
    </div>
  </div>

  <div class="container text-center" >
    <button type="submit" class="btn btn-dark">Submit</button>
  </div>

  
  
</form>




